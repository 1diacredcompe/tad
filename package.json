{
  "name": "tad",
  "productName": "Tad",
  "description": "Tabular data viewer",
  "version": "0.6.3",
  "author": "Antony Courtney <antony@antonycourtney.com> (http://www.antonycourtney.com)",
  "dependencies": {
    "babel-runtime": "^6.20.0",
    "bootstrap": "^3.3.7",
    "command-line-args": "^3.0.4",
    "command-line-usage": "^3.0.8",
    "console.table": "^0.7.0",
    "d3-array": "^1.0.2",
    "d3-fetch": "0.0.2",
    "es6-promise": "^4.0.5",
    "eslint-plugin-react": "^6.8.0",
    "fast-csv": "^2.3.0",
    "flow-bin": "^0.36.0",
    "gauge": "^2.7.2",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.1.1",
    "jsesc": "^2.3.0",
    "lodash": "^4.17.2",
    "oneref": "^0.2.1",
    "react": "^15.4.1",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^15.4.1",
    "slickgrid-es6": "^0.4.2",
    "sqlite": "^2.2.4",
    "sqlite-parser": "^1.0.0-rc3",
    "sqlite3": "^3.1.8",
    "through": "^2.3.8"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^17.0.2",
    "babel-loader": "^6.2.9",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "cpx": "^1.5.0",
    "css-loader": "^0.26.1",
    "electron": "^1.4.12",
    "electron-builder": "^10.6.1",
    "electron-packager": "^8.4.0",
    "electron-rebuild": "^1.4.0",
    "eslint": "^3.12.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-flowtype": "^2.29.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "file-loader": "^0.9.0",
    "image-webpack-loader": "^3.1.0",
    "jquery": "^3.1.1",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "node-gyp": "^3.4.0",
    "source-map-loader": "^0.1.5",
    "standard": "^8.6.0",
    "style-loader": "^0.13.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3",
    "webpack": "^1.14.0"
  },
  "main": "./build/main.bundle.js",
  "scripts": {
    "clean": "rm -rf build/*",
    "build:jquery": "mkdir -p build && cpx \"node_modules/jquery/dist/**/*\" build/js",
    "build:bootstrap": "mkdir -p build && cpx \"node_modules/bootstrap/dist/**/*\" build",
    "build:html": "mkdir -p build && cpx \"html/*\" build",
    "build-assets": "npm run build:bootstrap && npm run build:jquery && npm run build:html",
    "lint": "eslint .",
    "test-babel": "flow && standard src/*.js src/js/*.js src/js/**/*.js test/*.js && babel-node --presets es2015,react ./test/runAllTests.js | tap-spec",
    "test-babel:notap": "flow && standard src/*.js src/js/*.js src/js/**/*.js test/*.js && babel-node --presets es2015,react ./test/runAllTests.js",
    "test-babel:nostd": "flow && babel-node --presets es2015,react ./test/runAllTests.js | tap-spec",
    "test": "flow && standard src/*.js src/js/*.js src/js/**/*.js test/*.js && electron build/allTests.bundle.js | tap-spec",
    "runQuery": "babel-node --presets es2015,react ./test/runQuery.js",
    "start": "electron .",
    "electron-rebuild": "electron-rebuild -l -p",
    "packager": "electron-packager . --platform=darwin",
    "pack": "build --dir",
    "dist": "build",
    "webpack:watch": "webpack -d --watch",
    "build-release": "npm run dist && node ./tools/buildRelease.js",
    "csvimport": "electron build/csvimport.bundle.js"
  },
  "standard": {
    "ignore": [
      "webpack.config.js"
    ],
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ]
  },
  "build": {
    "appId": "com.antonycourtney.tad",
    "category": "DataVisualization",
    "files": [
      "build",
      "node_modules"
    ],
    "extraResources": [
      "tad.sh"
    ]
  },
  "directories": {
    "buildResources": "buildRes"
  },
  "license": "MIT"
}
