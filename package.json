{
  "name": "tad",
  "productName": "Tad",
  "description": "Tabular data viewer",
  "version": "0.6.4",
  "author": "Antony Courtney <antony@antonycourtney.com> (http://www.antonycourtney.com)",
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "bootstrap": "^3.3.7",
    "command-line-args": "^4.0.2",
    "command-line-usage": "^4.0.0",
    "console.table": "^0.8.0",
    "d3-array": "^1.1.1",
    "d3-fetch": "0.0.2",
    "electron-log": "^2.0.0",
    "es6-promise": "^4.1.0",
    "eslint-plugin-react": "^6.10.3",
    "fast-csv": "^2.4.0",
    "flow-bin": "^0.42.0",
    "gauge": "^2.7.3",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.2.1",
    "jsesc": "^2.4.0",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "oneref": "^0.3.1",
    "react": "^15.4.2",
    "react-dnd": "^2.2.4",
    "react-dnd-html5-backend": "^2.2.4",
    "react-dom": "^15.4.2",
    "slickgrid-es6": "^2.0.1",
    "sqlite": "^2.3.0",
    "sqlite-parser": "^1.0.0",
    "sqlite3": "git://github.com/antonycourtney/node-sqlite3.git",
    "through": "^2.3.8"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.0",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-3": "^6.22.0",
    "blue-tape": "^1.0.0",
    "cpx": "^1.5.0",
    "css-loader": "^0.27.3",
    "electron": "1.6.2",
    "electron-builder": "^16.0.1",
    "electron-packager": "^8.6.0",
    "electron-rebuild": "^1.5.7",
    "eslint": "^3.18.0",
    "eslint-config-standard": "^7.1.0",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.1.1",
    "file-loader": "^0.10.1",
    "image-webpack-loader": "^3.2.0",
    "jquery": "^3.2.1",
    "json-loader": "^0.5.4",
    "leaked-handles": "^5.2.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.2",
    "node-gyp": "^3.6.0",
    "source-map-loader": "^0.2.0",
    "standard": "^9.0.2",
    "style-loader": "^0.15.0",
    "tap-spec": "^4.1.1",
    "tap-summary": "^3.0.1",
    "tape": "^4.6.3",
    "webpack": "^2.3.0"
  },
  "main": "./build/main.bundle.js",
  "scripts": {
    "build:jquery": "mkdir -p build && cpx \"node_modules/jquery/dist/**/*\" build/js",
    "build:bootstrap": "mkdir -p build && cpx \"node_modules/bootstrap/dist/**/*\" build",
    "build:html": "mkdir -p build && cpx \"html/*\" build",
    "build-assets": "npm run build:bootstrap && npm run build:jquery && npm run build:html",
    "build-app": "npm run clean && npm run build-assets && node ./tools/buildApp.js",
    "build-release": "npm run dist && node ./tools/buildRelease.js",
    "clean": "rm -rf build/*",
    "csvimport": "electron build/csvimport.bundle.js",
    "dist": "build",
    "electron-rebuild": "electron-rebuild -l -p -f -w sqlite3",
    "lint": "eslint .",
    "pack": "build --dir",
    "packager": "electron-packager . --platform=darwin",
    "runQuery": "babel-node --presets es2015,react ./test/runQuery.js",
    "start": "electron .",
    "test": "flow && standard src/*.js src/js/*.js src/js/**/*.js test/*.js && ELECTRON_RUN_AS_NODE=true && electron build/allTests.bundle.js",
    "test-babel": "flow && standard src/*.js src/js/*.js src/js/**/*.js test/*.js && babel-node --presets es2015,react ./test/runAllTests.js",
    "test-node": "node build/allTests.bundle.js",
    "test-node-prof": "node --prof build/allTests.bundle.js",
    "test-prof": "babel-node --prof --presets es2015,react ./test/runAllTests.js",
    "webpack:watch": "webpack -d --watch"
  },
  "standard": {
    "ignore": [
      "webpack.config.js"
    ],
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ]
  },
  "build": {
    "appId": "com.antonycourtney.tad",
    "directories": {
      "buildResources": "buildRes"
    },
    "files": [
      "build",
      "node_modules"
    ],
    "extraResources": [
      "tad.sh"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": [
        "zip",
        "dmg"
      ]
    },
    "nodeGypRebuild": false,
    "nsis": {
      "perMachine": true
    }
  },
  "license": "MIT"
}
