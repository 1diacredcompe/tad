{
  "name": "tadviewer",
  "version": "0.9.1",
  "description": "Tabular data viewer library",
  "main": "dist/tadviewer.js",
  "types": "dist/tadviewer.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build:dir": "mkdirp dist",
    "build:html": "cp -r html/* dist",
    "build-assets": "npm run build:dir && npm run build:html",
    "build-dev": "npm run build-assets && webpack --mode development",
    "build-prod": "npm run build-assets && webpack --env prod --mode production",
    "watch": "webpack --mode development --watch",
    "watch-prod": "webpack --env prod --mode production --watch"
  },
  "keywords": [
    "relational",
    "sql",
    "database"
  ],
  "author": "Antony Courtney <antony@antonycourtney.com>",
  "license": "MIT",
  "dependencies": {
    "@blueprintjs/core": "^3.24.0",
    "@types/he": "^1.1.1",
    "aggtree": "../aggtree",
    "babel-plugin-flow-to-typescript": "^0.6.0",
    "byline": "^5.0.0",
    "clipboard": "^2.0.6",
    "console.table": "^0.10.0",
    "d3-array": "^2.4.0",
    "d3-fetch": "1.1.2",
    "eslint-plugin-react": "^7.19.0",
    "he": "^1.2.0",
    "immutable": "^3.8.2",
    "isomorphic-fetch": "^2.2.1",
    "jsesc": "^2.5.2",
    "oneref": "^0.7.0",
    "react": "^16.13.1",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dnd": "^10.0.2",
    "react-dnd-html5-backend": "^10.0.2",
    "react-dom": "^16.13.1",
    "react-select": "^3.1.0",
    "react-window-size-listener": "^1.5.4",
    "reltab": "../reltab",
    "slickgrid-es6": "^3.0.3",
    "url-regex": "5.0.0"
  },
  "devDependencies": {
    "@blueprintjs/icons": "^3.14.0",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-select": "^3.0.11",
    "ajv": "^6.12.0",
    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "^3.5.2",
    "file-loader": "^6.0.0",
    "image-webpack-loader": "^6.0.0",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "node-sass": "^4.13.1",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.4",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11"
  }
}
