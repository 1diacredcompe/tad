{
  "name": "EasyPivot",
  "version": "0.6.0",
  "dependencies": {
    "babel-runtime": "^6.18.0",
    "bootstrap": "^3.3.7",
    "command-line-args": "^3.0.3",
    "console.table": "^0.7.0",
    "d3-array": "^1.0.2",
    "d3-fetch": "0.0.2",
    "electron": "^1.4.10",
    "electron-compile": "^4.0.2",
    "es6-promise": "^4.0.5",
    "fast-csv": "^2.3.0",
    "flow-bin": "^0.36.0",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.1.1",
    "jsesc": "^2.2.0",
    "lodash": "^4.17.2",
    "slickgrid-es6": "^0.4.2",
    "sqlite": "^2.2.3",
    "sqlite3": "^3.1.8",
    "through": "^2.3.8"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^17.0.2",
    "babel-loader": "^6.2.8",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "cpx": "^1.5.0",
    "css-loader": "^0.26.0",
    "electron-compilers": "^4.0.5",
    "electron-rebuild": "^1.4.0",
    "eslint": "^3.11.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-flowtype": "^2.28.2",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "file-loader": "^0.9.0",
    "image-webpack-loader": "^3.0.0",
    "jquery": "^3.1.1",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "source-map-loader": "^0.1.5",
    "standard": "^8.6.0",
    "style-loader": "^0.13.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3",
    "webpack": "^1.13.3"
  },
  "main": "./build/main.bundle.js",
  "scripts": {
    "clean": "rm -rf build/*",
    "build:jquery": "mkdir -p build && cpx \"node_modules/jquery/dist/**/*\" build/js",
    "build:bootstrap": "mkdir -p build && cpx \"node_modules/bootstrap/dist/**/*\" build",
    "build:html": "mkdir -p build && cpx \"html/*\" build",
    "build": "npm run build:bootstrap && npm run build:jquery && npm run build:html",
    "lint": "eslint .",
    "test": "flow && standard src/*.js src/js/*.js src/js/**/*.js test/*.js && babel-node --presets es2015,react ./test/runAllTests.js | tap-spec",
    "test:notap": "flow && standard src/*.js src/js/*.js src/js/**/*.js test/*.js && babel-node --presets es2015,react ./test/runAllTests.js",
    "test:nostd": "flow && babel-node --presets es2015,react ./test/runAllTests.js | tap-spec",
    "runQuery": "babel-node --presets es2015,react ./test/runQuery.js",
    "start": "electron .",
    "electron-rebuild": "electron-rebuild"
  },
  "standard": {
    "ignore": [
      "webpack.config.js"
    ],
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ]
  },
  "license": "MIT"
}
