{
  "name": "",
  "version": "0.1.0",
  "dependencies": {
    "babel-runtime": "^6.18.0",
    "bootstrap": "^3.3.7",
    "lodash": "^4.16.4"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^16.0.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "cpx": "^1.5.0",
    "eslint": "^3.8.1",
    "eslint-config-standard": "^6.2.0",
    "eslint-plugin-flowtype": "^2.23.0",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-standard": "^2.0.1",
    "jquery": "^3.1.1",
    "json-loader": "^0.5.4",
    "source-map-loader": "^0.1.5",
    "standard": "^8.4.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.2",
    "webpack": "^1.13.2"
  },
  "scripts": {
    "clean": "rm -rf build/*",
    "build:jquery": "mkdir -p build && cpx \"node_modules/jquery/dist/**/*\" build/js",
    "build:bootstrap": "mkdir -p build && cpx \"node_modules/bootstrap/dist/**/*\" build",
    "build": "npm run build:bootstrap && npm run build:jquery",
    "lint": "eslint .",
    "test": "standard src/js/*.js src/js/**/*.js test/*.js && babel-node --presets es2015,react ./test/reltabTests.js | tap-spec"
  },
  "standard": {
    "ignore": [
      "webpack.config.js"
    ],
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ]
  }
}
