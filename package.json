{
  "name": "",
  "version": "0.1.0",
  "dependencies": {
    "babel-runtime": "^6.9.2",
    "bootstrap": "^3.3.7",
    "lodash": "^4.16.1"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-jest": "^15.0.0",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "cpx": "^1.5.0",
    "eslint": "^2.12.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-plugin-flowtype": "^2.18.1",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "jest": "^15.1.1",
    "jquery": "^3.1.0",
    "json-loader": "^0.5.4",
    "source-map-loader": "^0.1.5",
    "standard": "^8.0.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "webpack": "^1.13.1"
  },
  "scripts": {
    "clean": "rm -rf build/*",
    "lint": "standard",
    "build:jquery": "mkdir -p build && cpx \"node_modules/jquery/dist/**/*\" build/js",
    "build:bootstrap": "mkdir -p build && cpx \"node_modules/bootstrap/dist/**/*\" build",
    "build": "npm run build:bootstrap && npm run build:jquery"
  },
  "standard": {
    "ignore": [
      "webpack.config.js"
    ],
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ]
  }
}
