{
  "name": "tadweb-client",
  "version": "0.9.1",
  "description": "Tabular data viewer web app",
  "main": "dist/tadweb.bundle.js",
  "scripts": {
    "clean": "rimraf dist",
    "build:dir": "mkdirp dist",
    "build:html": "cp -r html/* dist",
    "build-assets": "npm run build:dir && npm run build:html",
    "build-dev": "npm run build-assets && webpack --mode development",
    "build-prod": "npm run build-assets && webpack --env prod --mode production",
    "watch": "webpack --mode development --watch",
    "watch-prod": "webpack --env prod --mode production --watch"
  },
  "keywords": [
    "relational",
    "sql",
    "database"
  ],
  "author": "Antony Courtney <antony@antonycourtney.com>",
  "license": "MIT",
  "dependencies": {
    "loglevel": "^1.6.8",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "oneref": "^0.7.0",
    "reltab": "../reltab",
    "tadviewer": "../tadviewer"
  },
  "devDependencies": {
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "ajv": "^6.12.0",
    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "^3.5.2",
    "file-loader": "^6.0.0",
    "image-webpack-loader": "^6.0.0",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "node-sass": "^4.13.1",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.4",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11"
  }
}
