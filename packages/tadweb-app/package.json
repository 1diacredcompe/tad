{
  "name": "tadweb-app",
  "version": "0.10.0",
  "description": "Tabular data viewer web app",
  "main": "dist/tadweb.bundle.js",
  "scripts": {
    "clean": "rimraf dist",
    "build:dir": "mkdirp dist",
    "build:html": "cp -r html/* dist",
    "build-assets": "npm run build:dir && npm run build:html",
    "build-dev": "npm run build-assets && webpack --mode development",
    "build-prod": "npm run build-assets && webpack --env prod --mode production",
    "build": "npm run build-prod",
    "prepublish": "npm run build",
    "test": "",
    "tsc": "tsc",
    "watch": "webpack --mode development --watch",
    "watch-prod": "webpack --env prod --mode production --watch"
  },
  "keywords": [
    "relational",
    "sql",
    "database"
  ],
  "author": "Antony Courtney <antony@antonycourtney.com>",
  "license": "MIT",
  "dependencies": {
    "loglevel": "^1.7.1",
    "oneref": "^0.8.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reltab": "^0.10.0",
    "tadviewer": "^0.10.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.16",
    "@types/react-dom": "^17.0.9",
    "ajv": "^6.9.1",
    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "^6.2.0",
    "file-loader": "^6.2.0",
    "image-webpack-loader": "^7.0.1",
    "less": "^3.13.1",
    "less-loader": "^10.0.1",
    "mkdirp": "^1.0.4",
    "node-sass": "^6.0.1",
    "resolve-url-loader": "^4.0.0",
    "sass-loader": "^12.1.0",
    "source-map-loader": "^3.0.0",
    "style-loader": "^3.2.1",
    "webpack": "^5.49.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.2"
  }
}
